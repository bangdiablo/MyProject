div class=\"layer_popup\">\n        <h3>사용자 종류 편집</h3>\n        <fieldset class=\"write_form\">\n            <legend>사용자 종류 편집</legend>\n            <p>\n                <span class=\"category\">\n                    <label for=\"searchPermission\">검색 허가</label>\n                </span>\n                <span class=\"field\">\n                    <span class=\"basic_select selectbox_ui\">\n                        <b class=\"selected_txt\"></b>\n                        <select id=\"searchPermission\">\n                            <option value=\"\">유저 아이디 선택</option>\n                            <option value=\"\" selected>Policy 2</option>\n                            <option value=\"\">ybgtest</option>\n                            <option value=\"\">ybgtest2</option>\n                        </select>\n                    </span>\n                </span>\n            </p>\n        </fieldset>\n\n        <div class=\"btn_set\">\n            <div class=\"right\">\n                <button type=\"button\" class=\"btn_basic cancel\">취소</button>\n                <button type=\"button\" class=\"btn_basic submit\">확인</button>\n            </div>\n        </div>\n        <button class=\"btn_close\"><img src=\"@/assets/images/component/btn_popup_close.png\" alt=\"닫기\" /></button>\n    </div>\n\n    <!-- 정책 삭제 -->\n    <div class=\"layer_popup\">\n        <h3>정책 삭제</h3>\n        <div class=\"msg warning\">\n            <h4><img src=\"@/assets/images/component/ico_lp_admin_warning.png\" alt=\"정책 삭제\" /></h4>\n            <p>\n                <strong>정책을 삭제합니다.</strong><br />\n                정책을 삭제하면 이 정책을 배정 받던 유저는 자신이 백업한 데이터의<br />\n                정책을 다시 선택할 필요가 있습니다. 진행하시겠습니까?\n            </p>\n        </div>\n\n        <div class=\"btn_set\">\n            <div class=\"right\">\n                <button type=\"button\" class=\"btn_basic cancel\">취소</button>\n                <button type=\"button\" class=\"btn_basic submit\">확인</button>\n            </div>\n        </div>\n        <button class=\"btn_close\"><img src=\"@/assets/images/component/btn_popup_close.png\" alt=\"닫기\" /></button>\n    </div>\n\n    <!-- 정책 삭제 - 삭제할 정책이 없을 때 -->\n    <div class=\"layer_popup\">\n        <h3>정책 삭제</h3>\n        <div class=\"msg warning\">\n            <h4><img src=\"@/assets/images/component/ico_lp_admin_warning.png\" alt=\"정책 삭제\" /></h4>\n            <p><strong>삭제할 정책이 없습니다.</strong></p>\n        </div>\n\n        <div class=\"btn_set\">\n            <div class=\"right\">\n                <button type=\"button\" class=\"btn_basic cancel\">취소</button>\n                <button type=\"button\" class=\"btn_basic submit\">확인</button>\n            </div>\n        </div>\n        <button class=\"btn_close\"><img src=\"@/assets/images/component/btn_popup_close.png\" alt=\"닫기\" /></button>\n    </div>\n\n    <!-- 정책 수정 -->\n    <div class=\"layer_popup\">\n        <h3>정책 수정</h3>\n        <fieldset class=\"write_form\">\n            <legend>정책 수정</legend>\n            <p>\n                <span class=\"category\">\n                    <label for=\"modifyPolicy\">할당 정책을 선택합니다.</label>\n                </span>\n                <span class=\"field\">\n                    <span class=\"basic_select selectbox_ui\">\n                        <b class=\"selected_txt\"></b>\n                        <select id=\"modifyPolicy\">\n                            <option value=\"\">사용자 정의</option>\n                            <option value=\"\" selected>Policy 2</option>\n                            <option value=\"\">ybgtest</option>\n                            <option value=\"\">ybgtest2</option>\n                        </select>\n                    </span>\n                </span>\n            </p>\n        </fieldset>\n\n        <div class=\"btn_set\">\n            <div class=\"right\">\n                <button type=\"button\" class=\"btn_basic cancel\">취소</button>\n                <button type=\"button\" class=\"btn_basic submit\">확인</button>\n            </div>\n        </div>\n        <button class=\"btn_close\"><img src=\"@/assets/images/component/btn_popup_close.png\" alt=\"닫기\" /></button>\n    </div>\n\n    <!-- 모달 배경 -->\n    <div class=\"modal_bg\"></div>\n  </div>\n\n\n</template>\n\n<script>\n  export default {\n\n    data() {\n      return {\n        list: [],\n        user_add_menu: {\n          display: 'none'\n        },\n        user_detail_menu: {\n          display: 'none'\n        },\n      }\n    },\n    computed: {\n\n      checkedAll: {\n\n          get() {\n\n              let count = 0;\n\n              for (let i = 0; i < this.list.length; i++) {\n\n                  if (this.list[i].checked) count++;\n              }\n\n              return this.list.length === count;\n          },\n          set(val) {\n\n              if (val)\n                  this.list.setAllObjProp('checked', true);\n              else\n                  this.list.setAllObjProp('checked', false);\n          }\n      },\n    },\n    methods: {\n\n        user_add_menu_on: function () {\n            this.user_add_menu.display = 'block'\n        },\n        user_add_menu_out: function () {\n            this.user_add_menu.display = 'none'\n        },\n        user_detail_menu_on: function () {\n            this.user_detail_menu.display = 'block'\n        },\n        user_detail_menu_out: function () {\n            this.user_detail_menu.display = 'none'\n        },\n\n        isAdmin(permission) {\n\n            return permission === \"A\"? true : false;\n        },\n        expressUsed(value, unit) {\n\n            unit = unit === undefined ? \" GB\" : \" 페이지\";\n\n            return value + unit;\n        },\n        expressLimit(value, unit) {\n\n            unit = unit === undefined ? \" GB\" : \" 페이지\";\n\n            if (value == 0)\n                value = \"제한없음\";\n            else\n                value += unit;\n\n            return value;\n        },\n        expressPermit(permit) {\n\n            switch(permit) {\n\n            case 0:\n                permit = \"백업전용\";\n                break;\n\n            case 1:\n                permit = \"검색전용\";\n                break;\n\n            case 2:\n                permit = \"전체\";\n                break;\n\n            default:\n                // do nothing\n            }\n\n            return permit;\n        },\n        showContext(event) {\n\n            let target = event.target;\n\n            if (target.tagName === \"I\")\n                target = target.parentElement;\n\n            target.getElementsByClassName(\"menu_layer\")[0].style.display = \"block\";\n        },\n        hideContext(event) {\n\n            let target = event.target;\n\n            if (target.tagName === \"I\")\n                target = target.parentElement;\n\n            target.getElementsByClassName(\"menu_layer\")[0].style.display = \"none\";\n        }\n    },\n    created() {\n\n      // TODO ajax로 list 조회 후\n      this.list = [\n        {\n          id: 1,\n          firstname: \"JY\",\n          lastname: \"Lee\",\n          permission: \"A\",\n          policy: \"\",\n          gnrlsto: 0,\n          gnrllimit: 0,\n          coldsto: 23,\n          coldlimit: 0,\n          ocr: 5,\n          ocrlimit: 0,\n          permit: 0,\n          checked: false\n        },\n        {\n          id: 2,\n          firstname: \"JK\",\n          lastname: \"Kang\",\n          permission: \"N\",\n          policy: \"\",\n          gnrlsto: 11,\n          gnrllimit: 300,\n          coldsto: 230,\n          coldlimit: 400,\n          ocr: 49,\n          ocrlimit: 500,\n          permit: 1,\n          checked: false\n        },\n        {\n          id: 3,\n          firstname: \"LA\",\n          lastname: \"Chu\",\n          permission: \"A\",\n          policy: \"\",\n          gnrlsto: 2,\n          gnrllimit: 0,\n          coldsto: 0.24,\n          coldlimit: 100,\n          ocr: 19,\n          ocrlimit: 50,\n          permit: 2,\n          checked: false\n        }\n      ]\n    }\n  }\n\n</script>\n\n<style scoped>\n  .fade-enter-active, .fade-leave-active {\n    transition: opacity 0.5s;\n  }\n\n  .fade-enter, .fade-leave-active {\n    opacity: 0;\n  }\n\n  #menu_layer_detail {\n    right: 1px;\n  }\n  .user_context {\n    top: 33px;\n    right: -1px;\n  }\n  .user_context_user_management {\n    top: -1px;\n    left: -162px;\n  }\n  .user_context_policy_management {\n    top: 30px;\n    left: -89px;\n  }\n  .user_context_user_limit {\n    top: 62px;\n    left: -149px;\n  }\n</style>\n"],"sourceRoot":"src/components/management/user_server"}]}