AAA,EAAA,IAAA,IAAA;AACA,WAAA,iBAAA,CAAA,IAAA,CAAA,UAAA,EAAA,aAAA,GAAA,IAAA,CAAA,SAAA;AACA,KAFA;AAIA,IAAA,OAAA,CAAA,GAAA,CAAA,wBAAA,EAAA,KAAA,iBAAA;;AACA,SAAA,IAAA,CAAA,IAAA,KAAA,iBAAA,EAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,eAAA,IAAA,GAAA,EAAA,KAAA,OAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,cAAA,IAAA,GAAA,EAAA,KAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,eAAA,IAAA,GAAA,EAAA,KAAA,KAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,cAAA,IAAA,GAAA,EAAA,KAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,WAAA,IAAA,GAAA,EAAA,KAAA,OAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,WAAA,IAAA,GAAA,EAAA,KAAA,KAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,WAAA,IAAA,GAAA,EAAA,KAAA,KAAA,GAAA,IAAA;AACA,UAAA,KAAA,iBAAA,CAAA,CAAA,EAAA,WAAA,IAAA,GAAA,EAAA,KAAA,KAAA,GAAA,IAAA;AACA,KApBA,CAsBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,GA/MA;;AAgNA,EAAA,OAAA,GAAA;AACA,IAAA,CAAA,CAAA,QAAA,CAAA,CAAA,KAAA,CAAA,UAAA,CAAA,EAAA;AACA,MAAA,MAAA,CAAA,+BAAA,CAAA,CAAA,gBAAA,CAAA;AACA,QAAA,KAAA,EAAA;AADA,OAAA,EADA,CAKA;;AACA,MAAA,MAAA,CAAA,eAAA,CAAA,CAAA,IAAA,CAAA,YAAA;AACA,QAAA,aAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CADA,CACA;AACA,OAFA;AAGA,KATA;AAWA;;AA5NA,CAAA","sourcesContent":["<template>\n    <div>\n        <div class=\"policy_write_form\">\n            <h3>{{ $t('policy.general.recommend_backup') }}</h3>\n            <ul class=\"pwf_check_list\">\n                <li class=\"all\">\n                    <label>\n                        <input type=\"checkbox\" v-model=\"selectAll\" @click=\"select_all\"/>{{\n                        $t('policy.general.select_all') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"desktop\"/>\n                        <img src=\"@/assets/images/sub/ico_pwf_desktop.png\" alt=\"\" class=\"ico\"/>{{$t('policy.general.desktop')\n                        }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"document\"/><img src=\"@/assets/images/sub/ico_pwf_document.png\"\n                                                                        alt=\"\" class=\"ico\"/>{{\n                        $t('policy.general.document') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"afile\"/><img src=\"@/assets/images/sub/ico_pwf_account.png\"\n                                                                     alt=\"\"\n                                                                     class=\"ico\"/>{{\n                        $t('policy.general.afile') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"bookmark\"/><img src=\"@/assets/images/sub/ico_pwf_bookmark.png\"\n                                                                        alt=\"\" class=\"ico\"/>{{\n                        $t('policy.general.bookmark') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"picture\"/><img src=\"@/assets/images/sub/ico_pwf_photo.png\"\n                                                                       alt=\"\"\n                                                                       class=\"ico\"/>{{\n                        $t('policy.general.picture') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"music\"/><img src=\"@/assets/images/sub/ico_pwf_music.png\" alt=\"\"\n                                                                     class=\"ico\"/>{{\n                        $t('policy.general.music') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"video\"/><img src=\"@/assets/images/sub/ico_pwf_video.png\" alt=\"\"\n                                                                     class=\"ico\"/>{{\n                        $t('policy.general.video') }}\n                    </label>\n                </li>\n                <li>\n                    <label>\n                        <input type=\"checkbox\" v-model=\"ebook\"/><img src=\"@/assets/images/sub/ico_pwf_nycard.png\" alt=\"\"\n                                                                     class=\"ico\"/>{{\n                        $t('policy.general.card') }}\n                    </label>\n                </li>\n            </ul>\n            <div class=\"pwf_fieldset\">\n                <h4>{{ $t('policy.general.fileLocation') }} <span\n                        class=\"note\">{{ $t('policy.general.file_note') }}</span>\n                </h4>\n                <table>\n                    <caption>{{ $t('policy.general.fileLocation') }}</caption>\n                    <colgroup>\n                        <col style=\"width:104px;\"/>\n                        <col style=\"width:*;\"/>\n                        <col style=\"width:40px;\"/>\n                    </colgroup>\n                    <tbody>\n                    <tr v-for=\"(row, index) in generalFileList\">\n                        <td>\n                            <button type=\"button\" class=\"btn_variable\" @click=\"insertVariable(row.bf_filepath, index)\">\n                                {{\n                                $t('policy.general.insert_var') }}\n                            </button>\n                        </td>\n                        <td><input type=\"text\" :title=\"$t('policy.general.folderLocation_insert')\"\n                                   v-model=\"row.bf_filepath\" :key=\"index\"/></td>\n                        <td v-if=\"index == 0\">\n                            <button class=\"btn_add\" @click=\"addRow\"><img src=\"@/assets/images/sub/btn_pwf_add.png\"\n                                                                         alt=\"추가\"/></button>\n                        </td>\n                        <td v-else>\n                            <button class=\"btn_delete\" @click=\"removeRow(index)\"><img\n                                    src=\"@/assets/images/component/btn_edit_delete.png\" alt=\"삭제\"/>\n                            </button>\n                        </td>\n                    </tr>\n                    </tbody>\n                </table>\n            </div>\n            <modals-container/>\n            <div class=\"pwf_fieldset\">\n                <h4>{{ $t('policy.general.folderLocation') }} <span\n                        class=\"note\">{{ $t('policy.general.file_note') }}</span>\n                </h4>\n                <table>\n                    <caption>폴더위치</caption>\n                    <colgroup>\n                        <col style=\"width:104px;\"/>\n                        <col style=\"width:104px;\"/>\n                        <col style=\"width:*;\"/>\n                        <col style=\"width:34px;\"/>\n                    </colgroup>\n                    <tbody>\n                    <tr v-for=\"(row, index) in generalFolderList\">\n                        <td>\n                            <button type=\"button\" class=\"btn_variable\" @click=\"insertFolder(row.bf_filepath, index)\">{{\n                                $t('policy.general.insert_var') }}\n                            </button>\n                        </td>\n                        <td v-if=\"row.bf_folderpath !='' \">\n                            <button type=\"button\" class=\"btn_variable\">{{ $t('policy.general.filterAdd') }}</button>\n                        </td>\n                        <td v-else>\n                            <button type=\"button\" class=\"btn_variable\" disabled=\"disabled\">\n                                {{$t('policy.general.filterAdd') }}\n                            </button>\n                        </td>\n                        <td><input type=\"text\" v-model=\"row.bf_folderpath\" :key=\"index\"\n                                   :title=\"$t('policy.general.folderLocation_insert')\"/></td>\n                        <td v-if=\"index == 0\">\n                            <button class=\"btn_add\" @click=\"addFolder\"><img src=\"@/assets/images/sub/btn_pwf_add.png\"\n                                                                            alt=\"추가\"/></button>\n                        </td>\n                        <td v-else>\n                            <button class=\"btn_delete\" @click=\"removeFolder(index)\"><img\n                                    src=\"@/assets/images/component/btn_edit_delete.png\"\n                                    alt=\"삭제\"/>\n                            </button>\n                        </td>\n                    </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import moment from 'moment'\n    import insertVariable from './policy_variable_popup'\n    import {EventBus} from \"../../../plugins/event-bus\";\n\n    export default {\n        data() {\n            return {\n                desktop: false,\n                document: false,\n                afile: false,\n                bookmark: false,\n                picture: false,\n                music: false,\n                video: false,\n                ebook: false,\n                rows: [],\n                selectAll: false,\n                selected: [],\n            }\n        },\n        components: {\n            appMyModal: insertVariable,\n        },\n        computed: {\n            policyData() {\n                return this.$store.state.management.policy.setPolicy.generalpolicydata\n            },\n            generalPolicyList() {\n                return this.$store.state.management.policy.policyDataList.generalPolicydata\n            },\n            coldPolicyList() {\n                return this.$store.state.management.policy.policyDataList.cold_policydata\n            },\n            generalFileList() {\n                return this.$store.state.management.policy.policyDataList.generalFile\n            },\n            generalFolderList() {\n                return this.$store.state.management.policy.policyDataList.generalFolder\n            },\n            setPolicyData(){\n                return this.$store.state.management.policy.setPolicy\n            }\n        },\n        watch: {\n            desktop() {\n                console.log(\"TESTE=====\")\n                if (!this.desktop == true) {\n                    for (let fm of this.setPolicyData.policydata) {\n                        if (fm.div == 'G' && fm.id == 2){\n                            fm.wallpaperchk = 'N';\n                        }\n                    }\n                }else{\n                    for (let fm of this.setPolicyData.policydata) {\n                        if (fm.div == 'G' && fm.id == 2){\n                            fm.wallpaperchk = 'Y';\n                        }\n                    }\n                }\n            },\n            document(){\n\n            },\n            afile(){\n\n            },\n            bookmark(){\n\n            },\n            picture(){\n\n            },\n            music(){\n\n            },\n            video(){\n\n            },\n\n        },\n        methods: {\n            addRow() {\n                this.generalFileList.push({\n                    bf_filepath: ''\n                })\n            },\n            addFolder() {\n                this.generalFolderList.push({\n                    bf_folderpath: ''\n                })\n            },\n            removeRow(index) {\n                this.generalFileList.splice(index, 1)\n            },\n            removeFolder(index) {\n                this.generalFolderList.splice(index, 1)\n            },\n            insertVariable(filepath, index) {\n                //MODAL\n                this.$modal.show(insertVariable, {\n                    filepath: filepath,\n                    modal: this.$modal,\n                    index: index,\n                    insert_div: 'file'\n                }, {\n                    name: 'dynamic-modal',\n                    width: '330px',\n                    height: '130px',\n                    draggable: false,\n                })\n            },\n            insertFolder(filepath, index) {\n                //MODAL\n                this.$modal.show(insertVariable, {\n                    filepath: filepath,\n                    modal: this.$modal,\n                    index: index,\n                    insert_div: 'folder'\n                }, {\n                    name: 'dynamic-modal',\n                    width: '330px',\n                    height: '130px',\n                    draggable: false,\n                })\n            },\n            select_all() {\n                this.selected = [];\n                if (!this.selectAll) {\n                    this.desktop = true;\n                    this.document = true;\n                    this.afile = true;\n                    this.bookmark = true;\n                    this.picture = true;\n                    this.music = true;\n                    this.video = true;\n                    this.ebook = true;\n\n                    console.log(\"descktop_value===\", this.desktop)\n                    for (let fm of this.setPolicyData.policydata) {\n                        if (fm.div == 'G' && fm.id == 2){\n                            fm.wallpaperchk = 'Y';\n                            fm.documentchk = 'Y';\n                            fm.acntnfilechk = 'Y';\n                            fm.bookmarkchk = 'Y';\n                            fm.imagechk = 'Y';\n                            fm.musicchk = 'Y';\n                            fm.videochk = 'Y';\n                            fm.ebookchk = 'Y';\n                        }\n                    }\n                    console.log(\"selectred====\", this.setPolicyData.policydata)\n\n                } else {\n                    this.desktop = false;\n                    this.document = false;\n                    this.afile = false;\n                    this.bookmark = false;\n                    this.picture = false;\n                    this.music = false;\n                    this.video = false;\n                    this.ebook = false;\n\n                    for (let fm of this.setPolicyData.policydata) {\n                        if (fm.div == 'G' && fm.id == 2){\n                            fm.wallpaperchk = 'N';\n                            fm.documentchk = 'N';\n                            fm.acntnfilechk = 'N';\n                            fm.bookmarkchk = 'N';\n                            fm.imagechk = 'N';\n                            fm.musicchk = 'N';\n                            fm.videochk = 'N';\n                            fm.ebookchk = 'N';\n                        }\n                    }\n                    console.log(\"selectred====\", this.setPolicyData.policydata)\n                }\n            },\n        },\n        created() {\n\n            EventBus.$on(\"variable_file_path\", data => {\n                this.generalFileList[data.file_index].bf_filepath = data.file_path;\n            });\n\n            EventBus.$on(\"variable_folder_path\", data => {\n                this.generalFolderList[data.file_index].bf_folderpath = data.file_path;\n            });\n\n            console.log(\"generalPolicyList>>>>>\", this.generalPolicyList)\n            for (let i in this.generalPolicyList) {\n                if (this.generalPolicyList[i].pd_wallpaperchk == \"Y\") this.desktop = true;\n                if (this.generalPolicyList[i].pd_documentchk == \"Y\") this.document = true;\n                if (this.generalPolicyList[i].pd_acntnfilechk == \"Y\") this.afile = true;\n                if (this.generalPolicyList[i].pd_bookmarkchk == \"Y\") this.bookmark = true;\n                if (this.generalPolicyList[i].pd_imagechk == \"Y\") this.picture = true;\n                if (this.generalPolicyList[i].pd_musicchk == \"Y\") this.music = true;\n                if (this.generalPolicyList[i].pd_videochk == \"Y\") this.video = true;\n                if (this.generalPolicyList[i].pd_ebookchk == \"Y\") this.ebook = true;\n            }\n\n            // if (this.$route.name == 'policy_edit') {\n            //     if (this.generalPolicyList.pd_wallpaperchk == \"Y\") this.desktop = true;\n            //     if (this.generalPolicyList.pd_documentchk == \"Y\") this.document = true;\n            //     if (this.generalPolicyList.pd_acntnfilechk == \"Y\") this.afile = true;\n            //     if (this.generalPolicyList.pd_bookmarkchk == \"Y\") this.bookmark = true;\n            //     if (this.generalPolicyList.pd_imagechk == \"Y\") this.picture = true;\n            //     if (this.generalPolicyList.pd_musicchk == \"Y\") this.music = true;\n            //     if (this.generalPolicyList.pd_videochk == \"Y\") this.video = true;\n            //     if (this.generalPolicyList.pd_ebookchk == \"Y\") this.ebook = true;\n            // }\n\n        },\n        mounted() {\n            $(document).ready(function ($) {\n                jQuery(\"article#contents.sub .content\").mCustomScrollbar({\n                    theme: \"minimal-dark\"\n                });\n\n                //셀렉트박스 UI\n                jQuery(\".selectbox_ui\").each(function () {\n                    initSelectBox(jQuery(this)) //비동기로 셀렉트 박스를 만들었을 경우 initSelectBox(jQuery(\"#아이디\")) 로 실행시켜줘야 함\n                })\n            });\n\n        }\n\n\n    }\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/views/management/policy"}]}