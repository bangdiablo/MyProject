KAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA;AA1BA,GAFA;;AAiCA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,aAAA,EAAA,EADA;AAEA,MAAA,MAAA,EAAA,KAAA,QAFA;AAGA,MAAA,KAAA,EAAA,EAHA;AAIA,MAAA,WAAA,EAAA;AAJA,KAAA;AAMA,GAxCA;;AAyCA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,GAAA;AACA,aAAA,KAAA,aAAA;AACA;;AAHA,GAzCA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,kBAAA,GAAA;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA,KAAA;AACA,KAHA;;AAIA,IAAA,iBAAA,GAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,kCAAA,EAEA,IAFA,CAEA,QAAA,IAAA;AACA,aAAA,aAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA;AAKA,KAVA;;AAWA,IAAA,YAAA,GAAA;AACA,UAAA,KAAA;;AACA,UAAA,SAAA,IAAA,KAAA,KAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,KAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,GAAA,SAAA;AACA;;AAEA,UAAA,IAAA,GAAA;AACA,QAAA,SAAA,EAAA,KAAA,KADA;AAEA,QAAA,UAAA,EAAA,KAAA,KAFA;AAGA,QAAA,KAAA,EAAA,KAAA;AAHA,OAAA;;AAKA,UAAA,KAAA,UAAA,IAAA,WAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,uBAAA,EAAA,IAAA;AACA;;AACA,UAAA,KAAA,UAAA,IAAA,iBAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,6BAAA,EAAA,IAAA;AACA;;AACA,UAAA,KAAA,UAAA,IAAA,eAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,2BAAA,EAAA,IAAA;AACA;;AACA,UAAA,KAAA,UAAA,IAAA,YAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,6BAAA,EAAA,IAAA;AACA;;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA,KAAA;AACA,KArCA;;AAsCA,IAAA,kBAAA,GAAA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,SAAA,EAAA,KAAA,KADA;AAEA,QAAA,UAAA,EAAA,KAAA;AAFA,OAAA,CADA,CAKA;AACA;AACA;;AA7CA,GA9CA;AA6FA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,CAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,GAAA;AACA;;AAHA,GA7FA;;AAkGA,EAAA,OAAA,GAAA;AACA,SAAA,iBAAA;AACA;;AApGA,CAAA","sourcesContent":["<template>\n\n    <!-- 변수 삽입 -->\n    <div class=\"layer_popup preview\" v-if=\"visible\">\n        <h3>변수 삽입</h3>\n        <p class=\"desc\">다음의 변수가 AOSBOX AI Plus의 표준 폴더 경로(장소)로 식별됩니다. 변수를 복사하려면 변수를 선택하고 \"변수를 삽입\"를 클릭합니다. </p>\n        <table class=\"board_list\">\n            <caption>{{title}}</caption>\n            <colgroup>\n                <col style=\"width:50px\"/>\n                <col style=\"width:150px\"/>\n                <col style=\"width:280px\"/>\n                <col style=\"width:280px\"/>\n            </colgroup>\n            <thead>\n            <tr>\n                <th scope=\"col\" colspan=\"2\">변수</th>\n                <th scope=\"col\">Mac OS X</th>\n                <th scope=\"col\">Windows</th>\n            </tr>\n            </thead>\n            <tbody>\n            <tr v-for=\"item in variableList\">\n                <td><input type=\"radio\" v-model=\"vcheck\" :value=\"item.bv_variable\"/></td>\n                <td class=\"left\">{{item.bv_variable}}</td>\n                <td>{{item.bv_macpath}}{{filepath}}</td>\n                <td>{{item.bv_windowspath}}</td>\n            </tr>\n            </tbody>\n        </table>\n        <ul class=\"desc_list\" style=\"margin-top:15px;\">\n            <li>전체 9건 중 1에서 9을 표시중</li>\n        </ul>\n        <div class=\"btn_set\">\n            <button v-if=\"insert_div == 'file'\" type=\"button\" class=\"btn_basic submit\" @click=\"sendVariable\">변수를 삽입\n            </button>\n            <button v-else type=\"button\" class=\"btn_basic submit\" @click=\"sendFolderVariable\">변수를 삽입</button>\n            <button type=\"button\" class=\"btn_basic cancel\" @click=\"$emit('update:visible', false)\">닫기</button>\n        </div>\n        <button class=\"btn_close\" @click=\"$emit('update:visible', false)\"><img\n                src=\"@/assets/images/component/btn_popup_close.png\"\n                alt=\"닫기\"/></button>\n    </div>\n\n</template>\n\n<script>\n    import {EventBus} from \"../../../plugins/event-bus\";\n\n    export default {\n        name: \"policy_variable_coldpopup\",\n        props: {\n            visible: {\n                type: Boolean,\n                require: true,\n                default: false\n            },\n            cold_modal: {\n                type: String,\n                require: false\n            },\n            title: {\n                type: String,\n                require: false,\n            },\n            filepath: {\n                type: String,\n                require: false,\n            },\n            insert_div: {\n                type: String,\n                require: false,\n            },\n            index: {\n                type: Number,\n                require: false\n            },\n            bf_id: {\n                type: Number,\n                require: false\n            },\n        },\n        data() {\n            return {\n                variable_list: [],\n                vcheck: this.filepath,\n                value: '',\n                checkedData: false,\n            }\n        },\n        computed: {\n            variableList() {\n                return this.variable_list\n            }\n        },\n        methods: {\n            handleWrapperClick() {\n                this.$emit('update:visible', false)\n            },\n            get_variable_list() {\n                axios.get('/management/policy_variablelist/'\n                )\n                    .then((response) => {\n                        this.variable_list = response.data\n                    })\n            },\n            sendVariable() {\n                var bfdiv;\n                if (undefined != this.bf_id) {\n                    bfdiv = this.bf_id\n                } else {\n                    bfdiv = undefined\n                }\n\n                let data = {\n                    file_path: this.value,\n                    file_index: this.index,\n                    bf_id: this.bf_id\n                }\n                if (this.cold_modal == 'ocr_modal') {\n                    EventBus.$emit('variable_ocr_filepath', data)\n                }\n                if (this.cold_modal == 'extension_modal') {\n                    EventBus.$emit('variable_extensionfile_path', data)\n                }\n                if (this.cold_modal == 'general_modal') {\n                    EventBus.$emit('variable_generalfile_path', data)\n                }\n                if (this.cold_modal == 'cold_modal') {\n                    EventBus.$emit('variable_coldfile_path_cold', data)\n                }\n                this.$emit('update:visible', false);\n            },\n            sendFolderVariable() {\n                let data = {\n                    file_path: this.value,\n                    file_index: this.index,\n                }\n                // EventBus.$emit('variable_coldfolder_path_cold', data)\n                // this.$emit('update:visible', false);\n            },\n        },\n        watch: {\n            vcheck(val) {\n                this.value = val\n            }\n        },\n        created() {\n            this.get_variable_list()\n        }\n    }\n</script>\n\n<style scoped>\n    .layer_popup {\n        display: block;\n    }\n</style>\n"],"sourceRoot":"src/views/management/policy"}]}